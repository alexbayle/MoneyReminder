<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.julienbonnetaud.moneyreminder" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>Money-Reminder</name>
    <description>
        Money Reminder, because good fences make good neighbours. :)
    </description>
    <author email="julien.bonnetaud@gmail.com" href="http://cordova.io">
        Julien Bonnetaud, Alexandre Bayle et Ali Abdennadher, IIM
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
</widget>
